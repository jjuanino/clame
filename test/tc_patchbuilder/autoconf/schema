# vim: set ft=conf:
# $id$

PERMS="0644"
OWNERS="root:daemon"

PERMS_DIR="0700"
OWNERS_DIR="nobody:nogroup"

EXEC_PERMS = "0750"
EXEC_OWNERS = "root:root"

dirdefaults $(PERMS_DIR) $(OWNERS_DIR)
notdirdefaults $(PERMS) $(OWNERS)

# directorios
d bin
d info
d man
d man/man1
d share
